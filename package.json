{
  "name": "password-generator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.17",
    "@types/node": "12.7.0",
    "@types/nouislider": "^9.0.5",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "@types/reactstrap": "^8.0.1",
    "formik": "^1.5.8",
    "node-sass": "^4.12.0",
    "nouislider": "^13.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "reactstrap": "^8.0.1",
    "source-map-explorer": "^2.0.1",
    "typescript": "3.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "analyze": "source-map-explorer 'build/static/css/*.css' 'build/static/js/*.js'",
    "analyze:build": "npm run build && npm run analyze",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/application/**/*.{js,jsx,ts,tsx}",
      "!src/application/serviceWorker.ts",
      "src/domain/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
